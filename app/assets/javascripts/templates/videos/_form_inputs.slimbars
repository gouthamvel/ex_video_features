.form-group
  label[for="video_title"]
    | Title
  br
  input#video_title[type="text" name="video[][title]"]
.form-group
  label[for="video_video_url"]
    | Video url
  br
  input#video_video_url[type="file" name="video_input_field" data-name="video[][video_url]"]
  .progress
    .progress-bar[aria-valuemax="100" aria-valuemin="0" aria-valuenow="60" role="progressbar" style="width: 0%;"]
